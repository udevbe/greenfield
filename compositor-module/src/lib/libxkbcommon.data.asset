        };
        row {
            top  = 49.5;
            left = 24.5;
            keys { <AB01>, <AB02>, <AB03>, <AB04>, <AB05> };
        };
        row {
            top  = 0;
            left = 150;
            keys { { <BKSP>, "DHAL" },
                   <AE06>, <AE07>, <AE08> };
            key.color = "grey20";
            keys { <AE09>, <AE10>, <AE11>, { <AE12>, "ALPL" } };
        };
        row {
            top  = 16.5;
            left = 174.5;
            keys { <AD06>, <AD07>, <AD08> };
            key.color = "grey20";
            keys { <AD09>, <AD10>, <AD11>, { <AD12>, "ALPL" } };
        };
        row {
            top  = 33;
            left = 150;
            keys { { <RTRN>, "DHAL" },
                   <AC06>, <AC07>, <AC08> };
            key.color = "grey20";
            keys { <AC09>, <AC10>, <AC11>, { <RTSH>, "DHAL" } };
        };
        row {
