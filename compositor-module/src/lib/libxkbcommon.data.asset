// The following code creates the "Cameroon key" phenomenon.

partial modifier_keys
xkb_symbols "semicolon_switch" {
	key <AC10> {
	symbols[Group1] = [ ISO_Level3_Latch,	colon,	semicolon,	colon ]};
	modifier_map Mod5	{ ISO_Level3_Latch };
};

