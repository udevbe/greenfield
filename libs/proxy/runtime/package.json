{
  "name": "@gfld/proxy-runtime",
  "version": "1.0.0-alpha.21",
  "type": "commonjs",
  "main": "dist/westfield-proxy-server.js",
  "typings": "types/westfield-proxy-server.d.ts",
  "description": "Wayland HTML5 runtime for Node.js",
  "files": [
    "dist/include",
    "dist/westfield-proxy-server.js",
    "types",
    "CMakeLists.txt",
    "native"
  ],
  "keywords": [
    "wayland",
    "html5",
    "weboscket",
    "compositor",
    "browser",
    "javascript",
    "ipc"
  ],
  "engines": {
    "node": ">=18"
  },
  "author": "Erik De Rijcke",
  "homepage": "https://www.greenfield.app",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/udevbe/greenfield/tree/master/libs/proxy/runtime"
  },
  "packageManager": "yarn@3.6.0",
  "scripts": {
    "build:native": "mkdir -p build && cmake -G Ninja -B./build -S./ && ninja -C ./build",
    "postinstall": "mkdir -p build && cmake -G Ninja -B./build -S./ && ninja -C ./build",
    "build": "tsc && yarn build:native",
    "clean": "rm -rf build/ dist/ types/"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.15.3",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "installConfig": {
    "hoistingLimits": "workspaces"
  }
